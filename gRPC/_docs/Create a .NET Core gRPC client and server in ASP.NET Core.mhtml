<pre><code class=3D"lang-csharp">using System;
using System.Net.Http;
using System.Threading.Tasks;
using Grpc.Net.Client;

namespace GrpcGreeterClient
{
    class Program
    {
        static async Task Main(string[] args)
        {
            // The port number(5001) must match the port of the gRPC server=
.
            using var channel =3D GrpcChannel.ForAddress("https://localhost=
:5001");
            var client =3D new Greeter.GreeterClient(channel);
            var reply =3D await client.SayHelloAsync(
                              new HelloRequest { Name =3D "GreeterClient" }=
);
            Console.WriteLine("Greeting: " + reply.Message);
            Console.WriteLine("Press any key to exit...");
            Console.ReadKey();
        }
    }
}
</code></pre></li>
